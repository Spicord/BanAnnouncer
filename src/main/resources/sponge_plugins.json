            "contributors": [
                {
                    "name": "Tini",
                    "description": "Lead Developer"
                }
            ],
            "dependencies": [
                {
                    "id": "spicord",
                    "version": "[5.3.1,)",
                    "load-order": "after",
                    "optional": false
                },
                {
                    "id": "libertybans",
                    "version": "[1.0.4,)",
                    "load-order": "after",
                    "optional": true
                }
            ]
        }
    ]
